  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],


  "main": "dist/main/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "web:dev": "webpack-dev-server --mode development",
    "web:prod": "npx http-server -p 3000 --proxy http://localhost:3000\\? static/main",
    "electron:dev": "electron-webpack dev -- ",
    "electron:dist": "npm run build && electron-webpack && electron-builder --mac --win --linux",
    "electron:dist:unpacked": "npm run dist --dir -c.compression=store -c.mac.identity=null",
    "start-build-time-profile": "webpack-dev-server --mode development --profile --progress",
    "icon:gen": "npx node build/icon-gen.js"
  },


  "devDependencies": {
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "devtron": "^1.4.0",
    "electron": "^8.0.2",
    "electron-builder": "^22.3.2",
    "sharp": "^0.24.1",
    "to-ico": "^1.1.5",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "create-servers": "^3.1.0",
    "electron-is-dev": "^1.1.0",
    "electron-webpack": "^2.7.4",
    "minimist": "^1.2.0",
    "portfinder": "^1.0.25",
    "readable-stream": "^2.3.7"
  }
}
